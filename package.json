{
  "name": "@dontuncleme/prettier",
  "version": "1.0.4",
  "description": "Prettier config",
  "author": "Siarhiej Samsona≈≠ <dontuncleme@gmail.com>",
  "license": "MIT",
  "keywords": [
    "prettier",
    "prettier-config"
  ],
  "main": "index.json",
  "scripts": {
    "editorconfig": "npx editorconfig-checker",
    "prettier": "npx prettier --write '**/*.{json,js,yml}'"
  },
  "lint-staged": {
    "**/*.{json,js,yml}": [
      "npx prettier --write"
    ]
  },
  "prettier": "./index.json",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "prettier": "^2.4.1"
  },
  "devDependencies": {
    "editorconfig-checker": "^4.0.2",
    "lint-staged": "^11.1.2",
    "simple-git-hooks": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dontuncleme/prettier.git"
  },
  "bugs": {
    "url": "https://github.com/dontuncleme/prettier/issues"
  },
  "homepage": "https://github.com/dontuncleme/prettier#readme"
}
